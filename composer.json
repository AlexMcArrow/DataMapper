            "@phpunit",
            "@psalm"
        ]
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AlexMcArrow\\DataMapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex McArrow",
            "email": "alex@mcarrow.ru",
            "homepage": "https://github.com/AlexMcArrow",
